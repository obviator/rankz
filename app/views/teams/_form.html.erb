<%= render 'shared/errors', obj: @team %>
<%= form_for [@tournament, @team], url: [@tournament, @team], remote: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.label :race %>
  <%= f.collection_select(:race_id, races.order(:name), :id, :name, { prompt: '<select>' }, class: "form-control") %>
<% end %>
