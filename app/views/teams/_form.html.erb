<%= form_for [@tournament, @team], url: [@tournament, @team], remote: true do |f| %>
  <%= render 'shared/errors', obj: @team %>
  <%= f.label :name %>
  <%= f.text_field :name, class: "form-control" %>
  <%= f.label :race %>
  <%= f.collection_select(:race_id, races, :id, :name, {} , class: "form-control") %>
<% end %>
